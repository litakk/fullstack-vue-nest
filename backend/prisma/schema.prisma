generator client {
  provider = "prisma-client-js"
  //output   = "../generated/prisma"

}

datasource db {
  provider = "postgresql"
  // URL теперь задаётся только через prisma.config.ts / PrismaClientOptions
}

model User {
  id        String   @id @default(nanoid())
  name      String
  email     String   @unique
  password  String

  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt      @map("updated_at")

  @@map("users")
}
